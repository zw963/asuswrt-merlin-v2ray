                    }
                ]
            }
        },
        {
            // è¯†åˆ«è¿™æ˜¯ä¸€ä¸ª DNS è¯·æ±‚ï¼Œ å¹¶å‘é€åˆ°å†…éƒ¨ DNS è¿›è¡ŒæŸ¥è¯¢.
            "tag": "dns-outbound",
            "protocol": "dns",
            "streamSettings": {
                "sockopt": {
                    "mark": 255
                }
            }
        },
        {
            "tag": "direct",
            "protocol": "freedom",
            "settings": {
                // è¿™é‡Œæˆ‘é€‰æ‹©åŠ ä¸Šè¯•è¯•, æä¸å¥½æˆ‘çš„æ–¹æ¡ˆ, freedom çœŸçš„åˆå»è®¿é—® dnsmasq
                // çš„ 53 ç«¯å£, åˆè¢«è½¬åˆ° V2ray çš„ DNS å‘¢.
                "domainStrategy": "UseIP"
            },
            "streamSettings": {
                "network": "tcp",  // è¿™æ˜¯é»˜è®¤å€¼
                "sockopt": {
                    "mark": 255
                }
            }
        },
        {
            "tag": "block",
            "protocol": "blackhole",
            "settings": {
                "response": {
                    "type": "http"
                }
            }
        }
    ],
    "policy": {
        "levels": {
            "0": {
                "bufferSize": 4
            }
        }
    }
}
HEREDOC

echo "Your's shadowsocks port: [0m[33m${ss_port}[0m"
echo "Your's shadowsocks encrypt method: [0m[33m${ss_encrypt_method}[0m"
echo "Your's shadowsocks password: [0m[33m${ss_password}[0m"
echo 'Congratulations, Deploy succssful!'
